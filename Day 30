def coinChange(coins, amount):
    dp = [amount + 1] * (amount + 1)
    dp[0] = 0
    for i in range(1, amount + 1):
        for coin in coins:
            if i - coin >= 0: 
                dp[i] = min(dp[i], 1 + dp[i - coin])
    if dp[amount] == amount + 1:
        return -1
    else:
        return dp[amount]
coins = [1, 2, 5]
amount = 11
print(coinChange(coins, amount))   
Output: 3
